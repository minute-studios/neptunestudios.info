{
  "name": "minuteearth.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vue-cli-service build --watch --no-clean\" \"bundle exec jekyll serve\"",
    "start": "npm run dev",
    "serve": "vue-cli-service serve",
    "serve:dist": "http-server _site",
    "build": "vue-cli-service build && JEKYLL_ENV=production bundle exec jekyll build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "buefy": "^0.8.2",
    "bulma": "^0.7.1",
    "core-js": "^3.1.4",
    "vue": "^2.6.10",
    "vue-parallaxy": "^1.1.1",
    "vue-youtube": "^1.3.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "http-server": "^0.11.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.3.2",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
